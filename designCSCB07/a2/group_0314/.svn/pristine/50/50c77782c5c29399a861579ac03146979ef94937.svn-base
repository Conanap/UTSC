public static class Echo {

	public static echoString(String output) {
		String outTokens[] = output.split(" *");
		String finalOut = "";
		for(int i = 0; i < outTokens.length; i++) {
			finalOut += outTokens[i];
			if(i != outTokens.length - 1)
				finalOut += "\n";
		}
		return finalOut;
	}

	private static echoFiles() {
	}

	public static echoEntireString(String output) {
		return output;
	}
}
